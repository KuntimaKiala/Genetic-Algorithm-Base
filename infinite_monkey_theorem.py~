from population import Population
import time 
def initialize(population_size=150, mutationRate = 0.01, target = "To be or not to be.") :
    population = Population()
    # create the population
    population.population_init(target, mutationRate, population_size)
    return population

def run(population,i):
    population.naturalSelection()
    population.generate()
    population.calcFitness()
    print(f"epochs {i}")
    print(population.getBest())